{
    "name": "ngsoft/curl",
    "description": "A wrapper around PHP Curl",
    "keywords": ["curl", "client", "http", "http-client"],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],

    "require-dev": {
        "nyholm/psr7": "*",
        "psr/http-factory": "^1.0",
        "psr/container": "*",
        "psr/log": "*"
    },
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "psr/http-client": "*",
        "fig/http-message-util": "*"
    },
    "provide": {
        "psr/http-client-implementation": "1.0"
    },
    "suggest": {
        "nyholm/psr7": "A PSR-7 and PSR-17 implementation to build your requests."
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\Curl\\": "src/"
        },
        "files": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
